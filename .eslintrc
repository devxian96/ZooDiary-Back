{
    "root": true,
    "env": {
        "node": true,
        "browser": false
    },
    "extends": [
        "eslint:recommended",
        "plugin:@typescript-eslint/recommended",
        "plugin:prettier/recommended"
    ],
    "plugins": [
        "prettier",
        "@typescript-eslint"
    ],
    "rules": {
        "no-console": "off",
        "no-debugger": "warn"
    }
}